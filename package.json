{
  "name": "jmc_coming_soon",
  "version": "1.0.0",
  "engines": {
    "node": "10.0.0",
    "yarn": "0.22.0",
    "npm": "5.6.0"
  },
  "description": "Website to announce that JMc website is coming soon (whenever soon is)",
  "main": "index.js",
  "author": "Jason McLaren",
  "license": "MIT",
  "scripts": {
    "purge:idea": "rm -r .idea",
    "watch:js": "npx babel --no-babelrc js/script.precomp.js --presets=es2015 --plugins transform-object-rest-spread  --out-file js/script.js --source-maps --watch",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server --browser='Google Chrome'",
    "start:local": "npm-run-all --parallel devserver watch:sass watch:js",
    "heroku-postbuild": "npm-run-all build:css compile:js",
    "start": "node server/server.js",
    "start:development": "nodemon server/server.js",
    "compile:js": "npx babel --no-babelrc js/script.precomp.js --presets=es2015 --plugins transform-object-rest-spread  --out-file js/script.js",
    "webpack": "webpack",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "npx": "^10.2.0",
    "path": "^0.12.7",
    "postcss": "^6.0.22"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.71.0",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "npx": "^10.2.0",
    "path": "^0.12.7",
    "postcss": "^6.0.22"
  }
}
